# machine network - DEPLOYMENT_LOCATION
# valid values are AMAZON, LOCAL (any deployment that is not AMAZON should get the value LOCAL
# DEPLOYMENT_LOCATION=AMAZON
# in both local and distributed environments
DEPLOYMENT_LOCATION=LOCAL
PROXY_HOST=proxy_host
PROXY_PORT=proxy_port
# to create a hybrid environment, with a swarm cluster on a private cloud behind proxy, use these vars
#JAVA_PROXY_HOST=web-proxy.us.softwaregrp.net
#JAVA_PROXY_PORT=8080
#JAVA_NO_PROXY="localhost|127.0.0.1|15.119.*.*|config-service|aos-web"
# Image version
TAG=latest
# Services ports
MAIN_PORT=8082
ACCOUNT_PORT=8081
POSTGRES_PORT=5432
CONFIG_SERVICE_PORT=8888
GATEWAY_PORT=8080
ACTIVE_PROFILE=prod
CONFIG_FILE_URI=/configservice/config-repo
